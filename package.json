    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^4.2.0",
    "eslint-config-concise": "^0.9.1",
    "eslint-config-concise-esnext": "^0.8.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "temp-write": "^3.3.0"
  },
  "repository": "ratson/sync-package-version",
  "license": "MIT",
  "files": [
    "*.md",
    "LICENSE",
