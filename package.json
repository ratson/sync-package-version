{
  "name": "sync-package-version",
  "version": "0.0.4",
  "description": "Sync version number from package.json",
  "bin": "lib/cli.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint --ext js,md .",
